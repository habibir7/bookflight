{
    "version": 2,
    "builds": [
        {
          "src": "package.json",
          "use": "@vercel/static-build",
          "config": { "distDir": "public" }
        }
      ],
      "outputDirectory": "public",
      "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ]
}